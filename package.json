{
  "name": "laudiolin-electron",
  "description": "A music player in Electron.",
  "version": "0.1.0",
  "main": "src/electron/main.ts",
  "scripts": {
    "build": "webpack",
    "electron": "cross-env ELECTRON_IS_DEV=0 electron dist/electron.js",
    "electron:dev": "cross-env ELECTRON_IS_DEV=1 nodemon",
    "lint": "eslint src/**/*.ts",
    "react": "webpack --config webpack/react.config.js",
    "react:dev": "webpack serve --config webpack/react.config.js --mode=development",
    "dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\"",
    "start": "npm run build && npm run electron"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "howler": "^2.2.3",
    "sass": "^1.54.9",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^20.1.1",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.6",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "webpack-merge": "^4.2.1"
  }
}
