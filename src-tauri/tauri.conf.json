{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Laudiolin",
    "version": "2.2.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "scope": ["**"]
      },
      "protocol": {
        "asset": true,
        "assetScope": ["**"]
      }
    },
    "bundle": {
      "active": true,
      "category": "Music",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "moe.seikimo.laudiolin",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Play music!",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": {
            "en-US": {
              "localePath": "bundler/strings.wxl"
            }
          },
          "template": "bundler/bundler.wxs",
          "license": "bundler/LICENSE.rtf"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://raw.githubusercontent.com/seiKiMo-Inc/Laudiolin/tauri-react-v2/src-tauri/updates.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk1OTQ0Q0Y0Q0NDQzAyRTQKUldUa0Fzek05RXlVbFZJV3l3VW5vRml0ekJHQW9ETkY4ekN6RTNGSUhXbVVjdkNuRm10dCtockMK"
    },
    "windows": [
      {
        "title": "Laudiolin",
        "maximized": false,
        "center": true,
        "width": 1224,
        "height": 768,
        "minWidth": 1200,
        "minHeight": 600,
        "resizable": true,
        "decorations": false
      }
    ],
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    }
  }
}
